{
    "name": "cacti-gui-maker",
    "version": "1.0.0",
    "author": "CactiFX <fxcacti@gmail.com>",
    "main": "dist/index.js",
    "description": "makes pack-ui for thumbnails easier.",
    "keywords": [],
    "license": "ISC",
    "scripts": {
        "start": "electron .",
        "build": "tsc",
        "package-windows": "electron-packager . CactiGUIMaker --platform=win32 --arch=x64 --out=release --overwrite --icon=build/icon.ico",
        "package-linux": "electron-packager . CactiGUIMaker --platform=linux --arch=x64 --out=release --overwrite --icon=build/icon.png"
    },
    "type": "module",
    "dependencies": {
        "@napi-rs/canvas": "^0.1.54",
        "express": "^4.19.2",
        "sharp": "^0.33.5",
        "unzipper": "^0.12.3"
    },
    "devDependencies": {
        "@electron/packager": "^18.3.4",
        "@types/express": "^4.17.21",
        "@types/node": "^22.5.1",
        "@types/unzipper": "^0.10.10",
        "electron": "^32.0.1",
        "tsx": "^4.19.0",
        "typescript": "^5.5.4"
    }
}
